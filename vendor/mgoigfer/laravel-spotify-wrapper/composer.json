{
    "name": "mgoigfer/laravel-spotify-wrapper",
    "description": "A PHP wrapper for Spotify's Web API. ",
    "keywords": [
        "laravel",
        "php",
        "spotify",
        "package"
    ],
    "homepage": "https://github.com/mgoigfer/laravel-spotify-wrapper",
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Mikel Goig",
            "email": "m@mikelgoig.com",
            "homepage": "https://mikelgoig.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "jwilsson/spotify-web-api-php": "^2.6"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Mgoigfer\\SpotifyWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mgoigfer\\SpotifyWrapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "extra": {
        "laravel": {
            "providers": [
                "Mgoigfer\\SpotifyWrapper\\SpotifyWrapperServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
