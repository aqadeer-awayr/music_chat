{
    "name": "rennokki/larafy",
    "description": "Larafy is a Laravel API wrapper for Spotify.",
    "keywords": [
        "laravel",
        "wrapper",
        "api",
        "spotify",
        "music",
        "songs",
        "playlists",
        "playlist",
        "track",
        "tracks",
        "album",
        "albums"
    ],
    "homepage": "https://github.com/rennokki/larafy",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Renoki",
            "email": "rennokki@gmail.com",
            "homepage": "https://twitter.com/rennokki",
            "role": "Developer"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "orchestra/database": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Rennokki\\Larafy\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rennokki\\Larafy\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rennokki\\Larafy\\LarafyServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
